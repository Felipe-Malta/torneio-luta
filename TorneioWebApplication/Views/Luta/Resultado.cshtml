@model Competidor
@{
    ViewData["Title"] = "Resultado";
}
<div class="text-center mt-2">
    <h1>
        Resultado
    </h1>
</div>
<div class="w-100" style="display:flex; justify-content:center;">
    <div class="card h-100 m-3 ps-0 pe-0" style="width: 22%;">
        <div class="card-title py-2 ps-2 border-0 border-bottom border-light bg-secondary bg-opacity-10 mb-0">
            Campeão
        </div>
        <div class="card-text p-3 bg-light">
            <div>
                <b>@Model.Nome</b>
            </div>
        </div>
    </div>
</div>
